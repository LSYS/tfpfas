  url       = "https://pubs.rsc.org/en/content/articlepdf/2023/em/d2em00002d",
  keywords  = "PFAS",
  language  = "en",
  issn      = "2050-7887, 2050-7895",
  doi       = "10.1039/D2EM00002D"
}

